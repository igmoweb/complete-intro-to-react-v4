{
  "name": "adopt-me",
  "version": "1.0.0",
  "description": "An app to teach you to write React",
  "main": "src/App.js",
  "scripts": {
    "test": "jest --silent",
    "test:watch": "npm test -- --watch",
    "test:update": "npm test -- -u",
    "test:coverage": "npm test -- --coverage",
    "format": "prettier --write \"src/**/*.{js,jsx}\"",
    "lint": "eslint \"src/**/*.{js,jsx}\" --quiet",
    "dev": "parcel src/index.html",
    "build": "parcel build --public-url ./dist/ src/index.html",
    "start": "npm run build && babel-node server.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/btholt/complete-intro-to-react-v4.git"
  },
  "author": "Brian Holt <btholt@gmail.com>",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/btholt/complete-intro-to-react-v4/issues"
  },
  "homepage": "https://github.com/btholt/complete-intro-to-react-v4#readme",
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-eslint": "^8.2.6",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "emotion": "^9.2.10",
    "eslint": "^5.3.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-import": "^2.13.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-react": "^7.10.0",
    "jest": "^23.6.0",
    "parcel-bundler": "^1.9.7",
    "prettier": "^1.14.2",
    "react-emotion": "^9.2.10",
    "react-loadable": "^5.5.0",
    "react-test-renderer": "^16.5.2"
  },
  "dependencies": {
    "@reach/router": "^1.1.1",
    "babel-cli": "^6.26.0",
    "express": "^4.16.3",
    "lodash": "^4.17.11",
    "moment": "^2.22.2",
    "petfinder-client": "^0.0.3",
    "react": "^16.4.2",
    "react-dom": "^16.4.2",
    "react-redux": "^5.0.7",
    "redux": "^4.0.0",
    "redux-thunk": "^2.3.0"
  }
}
